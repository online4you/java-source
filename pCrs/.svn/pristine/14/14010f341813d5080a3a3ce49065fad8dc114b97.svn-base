/* Solapas */
#solapas {
	width:100%;
	height:22px;
	overflow:hidden;
	position:absolute;
	top:0px;
	left:0px;
}
.capaSolapa {
	height:24px;
	float:left;
}
.enlaceSolapa {
	text-align:center;
	color:#FFFFFF;
	text-decoration:none;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 10px;
	cursor:pointer;
}
.lineaBajaSolapa {
	height:4px;
	width:100%;
	clear:both;
}
.solapaAbaIzq2 {
	float:left;
	background-image:url(img/corners/i.gif);
	width:5px;
	height:4px;
}
.solapaAbaIzq {
	background-image:url(img/corners/arriba_i.gif);
	background-position:bottom;
	background-repeat:no-repeat;
	width:8px;
	height:23px;
}
.solapaAbaDer {
	background-image:url(img/corners/arriba_d.gif);
	background-position:bottom;
	background-repeat:no-repeat;
	width:8px;
	height:23px;
}
.solapaDobleLinea {
	background-image:url(img/doblelinea.gif);
	background-repeat:repeat-x;
	background-position:bottom;
	height:23px;
}
.esquinaDer {
	float:right;
	background-image:url(img/corners/arriba_d.gif);
	background-repeat:no-repeat;
	height:4px;
	width:8px;
}
#panelSolapas {
	position:relative;
	width:100%;
	height:480px;
}
.solapaNormal {
	background-image:url(img/solapa.gif);
	background-repeat:no-repeat;
	height:23px;
	width:90px;
	padding-top:4px;
	overflow:hidden;
	text-align:center;
}

.solapaMarcada {
	background-image:url(img/solapaMarcada.gif);
	background-repeat:no-repeat;
	height:23px;
	width:100px;
	padding-top:4px;
	overflow:hidden;
	text-align:center;
	font-weight:bold;
}
.solapaCerrar {
	background-image:url(img/solapa.gif);
	background-repeat:no-repeat;
	height:23px;
	width:90px;
	padding-top:4px;
	overflow:hidden;
	text-align:center;
}
