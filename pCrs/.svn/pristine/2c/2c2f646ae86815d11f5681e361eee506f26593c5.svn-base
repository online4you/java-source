package com.photel.interfaces.BDL244;

import java.math.BigDecimal;
import java.util.GregorianCalendar;

public interface IBDLDiscount {

	public abstract int getUnitCount();

	public abstract void setUnitCount(int unitCount);

	public abstract int getPaxCount();

	public abstract void setPaxCount(int paxCount);

	public abstract GregorianCalendar getDateTimeFrom();

	public abstract void setDateTimeFrom(GregorianCalendar dateTimeFrom);

	public abstract GregorianCalendar getDateTimeTo();

	public abstract void setDateTimeTo(GregorianCalendar dateTimeTo);

	public abstract String getDescription();

	public abstract void setDescription(String description);
	
	public void setAmount(BigDecimal amount) ;

	public BigDecimal getAmount();

}