@charset "utf-8";
/* CSS Document */
body {
	font-family:Verdana, Geneva, sans-serif;
	background-color:#fff;
	color:#666666;
	font-size:11px;
}

div#contenidoFrame a.col_reserva {
	line-height:15px;
	text-transform:none;
}

.stars {
	background:url("img/starCian.gif") repeat-x scroll 0 0 transparent;
}

div.resultado h3 a:visited {
	color:#009294;
	font-size:17px;
	font-weight:normal;
	text-decoration:underline;
}

h2#capaTitulo, h1#capaTitulo {
	font-size:15px;
	font-weight:normal;
	color:#404F85;
	font-family:"Times New Roman", Times, serif;
}

a:link, a:visited {
	text-decoration:none;
}

div#capaPrincipal {
	background-color:#fff;
	width:920px;
}

div.lista {
	border:1px solid #BABABA;
}

div#capaCabecera {
	background-image:url(img/photoCastelldefels.jpg);
	background-position:bottom;
	background-repeat:no-repeat;
	height:310px;
	margin:0px 10px 10px 10px;
	border-bottom:1px solid #000;
}
img#logo {
	top:9px;
}
div#idiomas {
	margin-right:10px;
}

div#capaBuscador {
	border:1px solid #CBE4F0;
	background-color:#CBE4F0;
}

div#capaOfertas {
	border:1px solid #CBE4F0;
	background-color:#CBE4F0;
}
div#capaBuscador p, div#capaOfertas p {
	color:#1183B1;
	text-transform:uppercase;
	font-weight:bold;
}
div#capaBuscador span {
	display:block;
	clear:both;
}
div#capaBuscador span input {
	border:none;
}
div#capaLateral {
	margin-left:10px;
}
div#capaBuscador a.boton {
	float:right;
	height:30px;
	display:block;
	overflow:hidden;	
}
p.tituloCapa {
	color:#055D80;
	font-family:Arial;
	font-size:14px;
	font-weight:bold;
}
span.titulo_lista {
	border:1px solid #BABABA;
	color:#000;
	background-position:top right;
	height:15px;
	/*padding-top:1px;*/
	background-image:url(img/boton_desplegable.gif);
	line-height:15px;
}
a#afini, a#affin {
	background-image:url(img/calendar_90.gif);
	background-color:#fff;
	background-position:top right;
	border:1px solid #bababa;
	height:19px;
}

div#frameCabecera a#afini, div#frameCabecera a#affin {
	background-color:#fff;
	background-position:top right;
}

a.boton {
	width:95px;
	height:23px;
	color:#485CA1;
	font-family:"Times New Roman", Times, serif;
	font-size:14px;
	font-weight:bold;
	cursor:pointer;
	text-decoration:none;
	text-align:center;
	border:none;
	line-height:15px;
	letter-spacing:1px;
	font-weight:bold;
	background-color:#F0F2F8;
	background-image:url(img/button_dlg.gif);
	background-repeat:no-repeat;
	padding-left:5px;
	text-transform:capitalize;
	padding-top:3px;
	
}

a.col_reserva {
	height:17px;
	color:#fff;
	text-decoration:none;
	border:none;
	width:90px;
	padding:0px;
	letter-spacing:1px;
	font-weight:bold;
	background-color:#001e5e;
	background-image:url(img/izqBoton.gif);
	background-repeat:no-repeat;
	padding-left:10px;
}

div#principalFrame {
	background-color:#FFF;
	margin-top:15px;
}
#iframeContenido {
	border:1px solid #fff;
	background-color:#fff;
	width:653px;
}

table#resumen th {
	background-color:#344784;
	color:#fff;
	padding:2px;
}
table#resumen td {
	background-color:#F0F2F8;
	color:#333;
	padding:2px;
	min-height:18px;
}
table#resumen td.coluTotal {
	font-weight:bold;
}

table#datosPersonales {
	border:1px solid #BABABA;
	margin-top:15px;
	line-height:20px;
}

table#datosPersonales td.colu1 {
	white-space:nowrap;
	color:#333;
	padding-left:10px;
	padding-right:5px;
}
table#datosPersonales input, table#datosPersonales select, table#datosPersonales textarea {
	border:1px solid #BABABA;
}
table#datosPersonales td.colu1 span {
	color:#333;
}

h2#capaTitulo, h1#capaTitulo {
	margin:0px 0px 5px 0px;
	text-transform:uppercase;
	color:#404F85;
	text-decoration:none;
}
div#capaMonedas {
	top:0px;
}

div#nHabitaciones {
	border:1px solid #BABABA;
	padding:10px;
	color:#000;
}
div.lahabi {
	border:1px solid #BABABA; 
	padding:10px;
} 
div#capaPie {
	margin-top:20px; 
}
a.condi {
	color:#333;
}
a.condi:hover {
	color:#000;
	font-weight:bold;
}

div#frameCabecera {
	background-color:#F0F2F8; 
	color:#344784;
}

div.fotosHabi {
float:left;
width:210px;
}

div.habiHotel img.fotoHabitacion {
border:3px solid #FAA21C;
float:left;
height:auto;
margin-right:15px;
overflow:hidden;
width:190px;
max-width:190px;
}