
* { margin:0; }
body { background-color:#3cc2ff; font:11px Arial; color:black; }
.clear { clear:both; }

.frame {background-color:white;}
.frame .container{width:100%;padding-left:6px;}
.noframe .rightcontent{padding-right:0;}
.noframe .leftcontent{padding-left:0;}

.container{
width:932px;
background-color:white;
margin:0 auto;
padding-left:15px;
}
.logo{
float:left;
