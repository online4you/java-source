       <div id='capaMonedas'>
        <div id='monedas' class='capa_lista'>
            <span class='titulo_lista'><%=nombreMoneda%></span>
            <div id='listaMonedas' class="lista">
            <%if hayMonedas then
            for m=0 to ubound(RegMonedas,2)%>
               <a href="javascript:cambiaMoneda('<%=RegMonedas(RMISO,m)%>');"><%=RegMonedas(RMTradu,m)%></a>
            <%next
            end if 'hayMonedas%>
            </div> <!--lista-->
        </div> <!--monedas-->
       </div> <!--capamonedas-->
