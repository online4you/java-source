<script language="javascript" type="text/javascript">
function comparaD(cp){
	window.location="<%=MiPag%>?est=<%=est%>&todos=<%=todos%>&any=<%=any%>&cp="+cp.value+"&tipof=<%=tipof%>&slp=<%=slp%>&mes=<%=mes%>";
}
function todosLosHoteles(){
	window.location='<%=MiPag%>?est=<%=est%>&todos=-1&any=<%=any%>&cp=<%=compara%>&tipof=<%=tipof%>&slp=<%=slp%>&mes=<%=mes%>';
}
function cambiaTipoF(nuevo){
	window.location='<%=MiPag%>?est=<%=est%>&todos=<%=todos%>&any=<%=any%>&cp=<%=compara%>&tipof='+nuevo.value+'&slp=<%=slp%>&mes=<%=mes%>';
}

</script>
