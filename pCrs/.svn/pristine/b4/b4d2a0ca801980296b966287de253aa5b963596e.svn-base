@charset "utf-8";
/* CSS Document */

body {
	font-family:Verdana,Arial,Helvetica,sans-serif;
	font-size:11px;
	color:#000000;
	margin:0px;
	padding:0px;
}

div#principalFrame {
	padding-left:5px;
	padding-right:5px;
	height:auto;
	overflow:hidden;
	position:relative;
	min-height:300px;
	padding-bottom:10px;
	width:625px;
	margin:0px;
}


div#capaCabecera {
	background-image:url(img/logo_econotels.jpg);
	background-repeat:no-repeat;
	background-position:left 5px;
	height:110px;
}

div#capaBuscador {
	border:1px solid #0384D5;
}
#iframeContenido {
	border:1px solid #0384D5;
}
a#afini, a#affin {
	border:1px solid #0384D5;
	color:#333333;
}
span.titulo_lista {
	border:1px solid #0384D5;
	background-image:url(img/gifselectAzul.gif);
}
div.lista {
	border:1px solid #0384D5;
}
a.boton {
	background-color:#0384D5;
	border:1px solid #3196DC;
	color:#fff;
	line-height:18px;
}
a.col_reserva {
	background-color:#0384D5;
	border:1px solid #3196DC;
	color:#fff;
}

div.resultado h3 a:link, div.resultado h3 a:active, div.resultado h3 a:visited {
	color:#FF963D;
}