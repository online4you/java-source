<form action="" method="post" name="f1">
<table>
  <tr>
    <td><div align="center" class="tituloTabla">CONSULTAS SQL</div></td>
  </tr>
<%'if adminBoss then%>
	<tr>
    	<td><div align="center">Seleccionar Empresa:<br/>
			<select name="laempre" onChange="javascript:cargaEmpresa();">
				<option value="0">Todas</option>
					<%if hayempre then
