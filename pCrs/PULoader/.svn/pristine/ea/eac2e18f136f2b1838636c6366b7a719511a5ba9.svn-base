#### Local path from Product Update zip file ####
#### Path del fichero zip del Product Update ####
storagePath=/stuff/tmp/BDLDownloads/cvs/Extended/

#### This boolean value indicates if you need to download the zip file or not ####
#### Este valor booleano indica si se tiene que descargar el zip o no ####
doFTPDownload=false

#### Database information ####
#### Informacion de la base de datos ####

# Database type (0 = MySQL, 1 = Oracle, 2 = SQL Server, 3 = Access)
# Tipo de base de datos (0 = MySQL, 1 = Oracle, 2 = SQL Server, 3 = Access)
dbTypeIdentifier=0

# Database name
# Nombre de la base de datos
dbName=bdl

# Database server
# Servidor en que esta la base de datos
dbServerName=localhost

# Database user
# Usuario de base de datos
dbUser=bdl

# Database password
# Contrasenya de la base de datos
dbPassword=bdl


