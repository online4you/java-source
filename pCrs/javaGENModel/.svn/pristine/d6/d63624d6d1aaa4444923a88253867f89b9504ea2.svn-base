<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.photel.data.gen.ddbb.hibernate.pojo">

<class
		name="GenCache"
		table="GEN_CACHE"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="string"
			column="TRD_CRITERIA"
		/>
		<property
			name="TrdDetalle"
			type="stringClobType"
			column="TRD_DETALLE"
		/>

		<property
			name="TrdTimcre"
			column="TRD_TIMCRE"
			type="timestamp"
			not-null="false"
			length="7"
		/>

	</class>	
</hibernate-mapping>