			name="GtrDatcre"
			column="GTR_DATCRE"
			type="date"
			insert="false"
			not-null="true"
			length="7"
		/>
		<property
			name="GtrOricla"
			column="GTR_ORICLA"
			type="string"
			not-null="false"
			length="100"
