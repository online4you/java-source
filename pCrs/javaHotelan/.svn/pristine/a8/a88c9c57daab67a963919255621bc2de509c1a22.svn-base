<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.photel.data.hotelan.ddbb.hibernate.pojo">
	<class
		name="HibernateHotel"
		table="hotels"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		/>
		<property
			name="name"
			column="name"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="typeId"
			column="typeId"
			type="integer"
			not-null="false"
		/>
		<property
			name="typeDes"
			column="typeDes"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="catId"
			column="catId"
			type="integer"
			not-null="false"
		/>
		<property
			name="catDes"
			column="catDes"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="destinationId"
			column="destinationId"
			type="integer"
			not-null="false"
		/>
		<property
			name="destinationDes"
			column="destinationDes"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="zoneId"
			column="zoneId"
			type="integer"
			not-null="false"
			length="500"
		/>
		<property
			name="zoneDes"
			column="zoneDes"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="address"
			column="address"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="zipCode"
			column="zipCode"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="region"
			column="region"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="country"
			column="country"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="telephone"
			column="telephone"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="fax"
			column="fax"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="web"
			column="web"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="email"
			column="email"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="active"
			column="active"
			type="integer"
			not-null="false"
		/>
	</class>	
</hibernate-mapping>