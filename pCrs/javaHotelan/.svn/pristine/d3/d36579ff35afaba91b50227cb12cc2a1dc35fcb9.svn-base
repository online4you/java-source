<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="Response">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Cathegories"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Cathegories">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="Cathegory"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Cathegory">
    <xs:complexType>
      <xs:attribute name="Description" use="required"/>
      <xs:attribute name="Id" use="required"/>
    </xs:complexType>
  </xs:element>
</xs:schema>
